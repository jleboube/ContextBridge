{
  "name": "contextbridge",
  "version": "1.0.0",
  "description": "Persistent conversation memory system for AI platforms",
  "main": "src/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon src/server/index.js",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "babel src/server -d dist/server --copy-files",
    "start": "node dist/server/index.js",
    "test": "jest",
    "lint": "eslint src",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "passport-google-oauth20": "^2.0.0",
    "knex": "^2.5.1",
    "pg": "^8.11.3",
    "joi": "^17.9.2",
    "rate-limiter-flexible": "^2.4.2",
    "multer": "^1.4.5-lts.1",
    "marked": "^7.0.4",
    "turndown": "^7.1.2",
    "openai": "^4.0.0",
    "axios": "^1.5.0",
    "uuid": "^9.0.0",
    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "react-hot-toast": "^2.4.1",
    "@heroicons/react": "^2.0.18",
    "react-hook-form": "^7.45.4",
    "@hookform/resolvers": "^3.3.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "vite": "^4.4.9",
    "@vitejs/plugin-react": "^4.0.4",
    "tailwindcss": "^3.3.3",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "lucide-react": "^0.279.0",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "@babel/core": "^7.22.20",
    "@babel/cli": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "jest": "^29.7.0",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}